  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint",
      "prettier --write",
      "jest --bail --findRelatedTests",
      "git add"
    ],
    "*.js": [
      "eslint",
      "prettier --write",
      "jest --bail --findRelatedTests",
      "git add"
    ]
  },
  "license": "MIT"
}
